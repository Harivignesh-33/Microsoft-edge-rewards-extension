{
    "manifest_version": 3,
    "name": "Microsoft Rewards Auto Searcher",
    "version": "1.0",
    "description": "Automates searches for Microsoft Rewards.",
    "permissions": ["tabs", "storage", "activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
  "default_popup": "popup.html",
  "default_icon": "icon.png",
  "default_title": "My Extension",
  "default_area": "navbar",
  "default_width": 300
}

  }
  